    id="navHomeTab",
    data-toggle="tab",
    href="#navHome",
    role="tab",
    aria-controls="navHome",
    aria-selected="true"
  )
    | Home
  a.nav-item.nav-link.not-smooth.text-normal(
    id="navProfileTab",
    data-toggle="tab",
    href="#navProfile",
    role="tab",
    aria-controls="navProfile",
    aria-selected="false"
  )
    | Profile
