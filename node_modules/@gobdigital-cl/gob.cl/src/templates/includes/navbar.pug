extends ../doc.pug

block content  
  section#navbar.doc-section
    header.doc-section-header
      h2.
        Navegación
        #[a(href='#navbar') #]
    .doc-section-body
      p.
        A continuación, una lista de los navbars disponibles para utilizar.
      div#light.doc-subsection
        .doc-subsection-header
          h3 Light #[a(href='#light') #]
        .doc-subsection-body
          .bg-gray-100
            p.
              Si el navbar es #[code .fixed-top] debe usar la clase
              #[code .with-fixed-navbar] en el main
            .p-2
              include ../examples/navbar-light
            pre(data-src='examples/navbar-light.html')
      div#dark.doc-subsection
        .doc-subsection-header
          h3 Dark #[a(href='#dark') #]
        .doc-subsection-body
          p.
            Si el navbar es #[code .fixed-top] debe usar la clase
            #[code .with-fixed-navbar] en el main
