  body.contingency-closed(data-main="main")
    header
      // Fixed navbar
      nav.navbar.navbar-light.navbar-expand-xl.fixed-top
        .container
          a.navbar-brand(href='/').mr-auto
            i.fa.fa-spinner.fa-spin.page-loading-icon
            img(src='img/gob-header.svg')
          .section-header
            - var hideReadSpeaker = true;
            include includes/components/toolbar
          button.navbar-toggler.collapsed.d-md-none.d-lg-none.d-xl-none(
            type='button',
            data-toggle='collapse',
            data-target='#navbarCollapse',
            aria-controls='navbarCollapse',
            aria-expanded='false',
            aria-label='Toggle navigation'
          )
            span.navbar-toggler-icon
          #navbarCollapse.navbar-collapse.collapse.max-vh-100.overflow-auto
            ul.navbar-nav.ml-auto.d-md-none.d-lg-none.d-xl-none
              each link in docSidebar
                li.nav-item.align-items-start.flex-column
                  a.nav-link(
                    href=link.page
                  )
                    | #{link.title}

    main.with-fixed-navbar
      include examples/contingency
      include examples/redirecting-1
      include examples/redirecting-2
      section.section-jumbotron.py-3
        .container
          .section-body
            img(src='img/gob-home.svg')
            h1.line
              | Framework kit Gobierno
      section.section
        .container
          .row
            .col-12.col-md-3.bd-sidebar.d-none.d-md-block.d-lg-block.d-lg-block
              include includes/sidebar
            .col-12.col-md-9
              block content
                include includes/getting-started
    footer.min-hight-lg-auto
      .container
        a.footer-logo(href='#')
          img.footer-default-logo(src='img/gob-footer.svg')
          img.footer-mobile-logo(src='img/gob-footer-mobile.svg')
        .row
          .col-sm-8
            .row
              .col-md-6
                a.link(href='https://digital.gob.cl/', target='_blank')
                  | División de Gobierno digital
              .col-md-6
                a.link(href='https://www.gob.cl/', target='_blank') Gob.cl

    script(src='https://code.jquery.com/jquery-3.5.1.min.js' integrity='sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=' crossorigin='anonymous')
